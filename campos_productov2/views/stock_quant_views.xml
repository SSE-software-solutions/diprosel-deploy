<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="stock_quant_search_view_inherit" model="ir.ui.view">
        <field name="name">stock.quant.search.view.inherit.business.sector</field>
        <field name="model">stock.quant</field>
        <field name="inherit_id" ref="stock.quant_search_view"/>
        <field name="arch" type="xml">
            <xpath expr="//filter[@name='company']" position="after">
                <filter string="Giro de Negocios" name="sector" context="{'group_by': 'sector_id'}"/>
            </xpath>
        </field>
    </record>
</odoo>
